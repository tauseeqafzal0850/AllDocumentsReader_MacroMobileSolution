<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="NewFontStyle" parent="android:Widget.TextView">
        <item name="android:textSize">@dimen/_16ssp</item>
    </style>

    <style name="dialogtheme"></style>
    <style name="textHeading">
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">@dimen/_7sdp</item>
        <item name="android:textSize">@dimen/_14sdp</item>
    </style>
    <style name="MyAlertDialogTheme" parent="Base.Theme.AppCompat.Light.Dialog.Alert">
        <item name="windowFixedWidthMajor">90%</item>
        <item name="windowFixedWidthMinor">90%</item>
    </style>
    <style name="dialogTextStyle">
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="lineHeight">@dimen/_18sdp</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="regularTextStyle">
        <item name="fontFamily">@font/roboto</item>
        <item name="lineHeight">@dimen/_14sdp</item>
        <item name="android:lineSpacingExtra">@dimen/_5sdp</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:textColor">@color/white</item>

    </style>
</resources>